<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- Type 별칭 미리 정의하기 -->
	<typeAliases>
		<typeAlias type="com.gura.myproject.file.dto.FileDto" alias="fileDto"/>
		<typeAlias type="com.gura.myproject.cafe.dto.CafeDto" alias="cafeDto"/>
		<typeAlias type="com.gura.myproject.users.dto.UsersDto" alias="usersDto"/>
	</typeAliases>
	
	

	<!-- sql 문을 정의하고 있는 xml 문서 정보 -->
	<mappers>
		<mapper resource="com/gura/myproject/mybatis/FileMapper.xml"/>
		<mapper resource="com/gura/myproject/mybatis/CafeMapper.xml"/>
		<mapper resource="com/gura/myproject/mybatis/UsersMapper.xml"/>
	</mappers>
</configuration>




